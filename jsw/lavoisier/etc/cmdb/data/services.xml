<?xml-stylesheet type="text/xsl" href="services.xsl"?>
<families><!-- xmlns="http://software.in2p3.fr/cmdb/services.xsd" -->
    <!-- essential|important|necessary (red|orange|green) -->
    <family name="BDII">
        <service name="SBDII"/>
        <service name="TBDII"/>
    </family>
    <family name="CE"/>
    <family name="CCTREQS">
        <service name="CCTREQS">
            <depends on="HPSS" criticality="essential"/>
        </service>
        <service name="CCTREQS_XROOTD">
            <depends on="HPSS" criticality="essential"/>
        </service>
    </family>
    <family name="DCACHE">
        <service name="DCACHE_LCG" url="https://svn.in2p3.fr/dCache/trunk/config/lcg/config.xml?revision=HEAD&amp;content-type=text%2Fplain">
            <depends on="LHCONE" criticality="important"/>
            <depends on="LHCOPN" criticality="important"/>
        </service>
        <service name="DCACHE_EGEE" url="https://svn.in2p3.fr/dCache/trunk/config/egee/config.xml?revision=HEAD&amp;content-type=text%2Fplain"/>
        <depends on="AFS" criticality="essential"/>
        <depends on="CRL" criticality="essential"/>
        <depends on="CCTREQS" criticality="important"/>
        <depends on="CCNAGIOS" criticality="necessary"/>
        <depends on="HPSS" criticality="important"/>
    </family>
    <family name="IRODS"/>
    <family name="LFC"/>
    <family name="MYPROXY"/>
    <family name="MYSQL"/>
    <family name="NAGIOS">
        <service name="CCNAGIOS"/>
    </family>
    <family name="TELECOM">
        <service name="LHCONE"/>
        <service name="LHCOPN"/>
    </family>
    <family name="OPENSTACK"/>
    <family name="ORACLE"/>
    <family name="POSTGRESQL"/>
    <family name="SPS"/>
    <family name="TSM"/>
    <family name="VOBOX"/>
    <family name="VMWARE"/>
    <family name="WEB"/>
    <family name="XROOTD">
        <service name="XROOTD">
            <depends on="CCTREQS_XROOTD" criticality="essential"/>
        </service>
        <service name="XROOTD_LCG">
            <depends on="CCTREQS_XROOTD" criticality="important"/>
        </service>
        <service name="XROOTD-PROXY-02">
            <depends on="CCTREQS_XROOTD" criticality="important"/>
        </service>
        <depends on="AFS" criticality="essential"/>
        <depends on="CRL" criticality="essential"/>
    </family>
    <!-- services without a family -->
    <service name="ACSLS"/>
    <service name="AFS"/>
    <service name="CRL"/>
    <service name="DNS"/>
    <service name="HPSS">
        <depends on="AFS" criticality="essential"/>
    </service>
    <service name="FLEXLM"/>
    <service name="FRONTIER"/>
    <service name="GE"/>
    <service name="NIS"/>
    <service name="RFIO"/>
</families>